{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "checkJs": false,
    "jsx": "react",
    "baseUrl": "src",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@Pages/*": ["Pages/*"],
      "@Redux/*": ["Redux/*"]
    },
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src", "media.d.ts"],
  "exclude": ["node_modules"]
}
