{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "checkJs": true,
    "jsx": "react",
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@assets/*": ["assets/*"],
      "@pages/*": ["Pages/*"]
